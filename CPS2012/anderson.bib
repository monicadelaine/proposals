@article{Duke2006,
abstract = {Course: Introduction to Mobile Robot Programming (for second-semester freshmen) Requirements: one semester of programming as a prerequisite, and another as a corequisite The robotic platform used is the AmigoBot from ActivMedia. A standard Linux-based laptop. Software: Java and the Eclipse IDE. Curriculum: First half semester: nonlinear. Robot characterization, reactive control, open- and closed-loop control, precision movement, and learning via operant conditioning Second half semester: progressive. Begin by having the students build primitives to map precision movement into movement commands useful for sensing and navigating a maze and bounding error accumulation. The next step is sequential planning, in which the robot must generate and execute its own plan for solving an arbitrary maze given a known starting point, goal and maze. Finally, information is hidden, and mazes must be solved both in the case where the starting position of the robot is unknown as well as in the case where the maze is not known. Logistics: one instructor, 2 TAs. 26 students, 13 teams, 2 of each. Each team uses one laptop and share time on 8 robots. Weekly cycle work. Observations: * Robotics is a fertile field for generating interesting computational problems which are interesting but do not require extensive computer science backgrounds to approach. * Competition is good. * Keeping teams fluid for much of the semester improved the students' teamwork skills by letting them work with a variety of personalities and coding styles. * Making the code repository available is a valuable method for teaching the students how to read, understand, and debug code, and to find examples of design patterns which work well. It also helps to keep students from falling irreparably behind as lab assignments begin to build on previous work. * When debugging, students are forced to come up with strategies for making behavior triggers visible and breaking down the problem into smaller parts. * To be effective, teaching this course requires a significant amount of time invested in interactions between instructors and small groups},
author = {Duke, David and Carlson, Justin and Thorpe, Chuck},
file = {:Users/manderson/Documents/Mendeley Desktop/2006 - Duke, Carlson, Thorpe - \{Robotics in Early Undergraduate Education\}.pdf:pdf},
journal = {AAAI},
keywords = {*file-import-10-04-21,education,robotics},
publisher = {American Association for Artificial Intelligence},
title = {{\{Robotics in Early Undergraduate Education\}}},
url = {citeulike-article-id:7046113},
year = {2006}
}
