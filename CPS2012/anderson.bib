@article{Koenig2001,
abstract = {Ant robots are simple creatures with limited sensing and computational capabilities. They have the advantage that they are easy to program and cheap to build. This makes it feasible to deploy groups of ant robots and take advantage of the resulting fault tolerance and paral- lelism. We study, both theoretically and in simulation, the behavior of ant robots for one-time or repeated coverage of terrain, as required for lawn mowing, mine sweeping, and surveil- lance. Ant robots cannot use conventional planning methods due to their limited sensing and computational capabilities. To overcome these limitations, we study navigation methods that are based on real-time (heuristic) search and leave markings in the terrain, similar to what real ants do. These markings can be sensed by all ant robots and allow them to cover terrain even if they do not communicate with each other except via the markings, do not have any kind of memory, do not know the terrain, cannot maintain maps of the terrain, nor plan complete paths. The ant robots do not even need to be localized, which completely eliminates solving difficult and time-consuming localization problems. We study two simple real-time search methods that differ only in how the markings are updated. We show experimentally that both real-time search methods robustly cover terrain even if the ant robots are moved without re- alizing this (say, by people running into them), some ant robots fail, and some markings get destroyed. Both real-time search methods are algorithmically similar, and our experimental results indicate that their cover time is similar in some terrains. Our analysis is therefore sur- prising. We show that the cover time of ant robots that use one of the real-time search methods is guaranteed to be polynomial in the number of locations, whereas the cover time of ant ro- bots that use the other real-time search method can be exponential in (the square root of) the number of locations even in simple terrains that correspond to (planar) undirected trees.},
author = {Koenig, S and Szymanski, B and Liu, Y},
file = {:Users/manderson/Documents/Mendeley Desktop/2001 - Koenig, Szymanski, Liu - Efficient and inefficient ant coverage methods.pdf:pdf},
journal = {Annals of Mathematics and Artificial Intelligence},
keywords = {*file-import-10-04-21},
number = {1},
pages = {41----76},
title = {{Efficient and inefficient ant coverage methods}},
url = {citeulike-article-id:7046313},
volume = {31},
year = {2001}
}
@article{Wolz2001,
abstract = {Teaching design and project management with lego RCX robots The course unit described here took time out from a Java-based CS 1 course to give students practical experience in programming a small robot in a highly restricted setting. The unit "Robot Planning and Design" had three goals: 1 . Provide a highly restricted environment in which mindless re-testing could not take place, 2. Break away from preconceived notions about what the fundamental tools were, 3. Make the experience sufficiently rewarding in and of itself, so that negative outcomes (failed attempts) provided significant lessons. 2 robots, 20 students, limit use Our goals, explicitly stated in the lab assignment were to: * teach students first hand what it means to plan out a project, * give students experience working with "real world" devices, * give students insight into the nature of programming languages. The Lego RCX Assignment: Week 1: Learning the Basics: During the first week, the students were introduced to the RCX language, the tutorials and the environment on personal computers that supports it. Week 2: Implementing a Problem Solution: team work together and write RCX code to navigate the robot from a start position in one room, to an end position in the other. Your robot just has to get there, but it is even better if it can detect that it has reached the end position. Week 3: One Shot Testing: the robots were made available to the students. The groups were encouraged to preplan their two-hour experience, and especially to have a navigation program ready for downloading before the session. They were allowed to modify their program after the session, but could not retest. Week 4: Solutions were demonstrated in a lab session at the beginning of week four. The discussion kept returning to two topics: (1) the failure of the robotic devices to behave predictably, and (2) the need to program reactively (based on events) rather than sequentially. Students identified three sources for learning RCX code: the on-line, multi-media tutorial, their classmates, and simple experimentation. Students also expressed in some form insight into the distinction between sequential and reactive programming: the early experimentation with robot parts had taught them that rigid deterministic solutions would not work. The results of the robot trials are summarized as follows: * One robot managed to make it three rooms away by properly detecting doors. It finally failed when it couldn't turn adequately to find a door. It kept just missing the door, first on the left, then on the right. This, the most successful robot was the result of significant attention to design using systematic methods promoted in class. It is possible, that given unlimited time (e.g. more than 10 minutes) it might have found its way to the goal. * One robot correctly adjusted to corners, turning in small increments, by backing up a bit, and moving forward until it found the wall again. Unfortunately, it could not detect an open door unless it simply stumbled through it. This group suffered from the problem of over-engineering one component. Although their robot was best at turning, the light sensing procedure (needed to walk through doors) had never been adequately tested in conjunction with the other procedures for turning and moving forward. * One robot climbed the walls due to the speed with which the motors were set. The team had not anticipated that the touch sensor event used to detect an encounter with a wall would be thwarted because of the speed of the robot. In their two-hour testing period they did not have time to try bumping into things to see what would happen. * One robot, when it encountered a corner turned into the corner rather than out of it. We later realized that this group had tested its software with the motors reversed. Their solution, jerry-rigged during the testing period, failed completely because the robots were rebuilt (after both were accidentally disassembled) before the demonstration. * The most beloved robot (and the first to be emonstrated) backed up against the wall by the tarting position, moved into the exact center of the room and proceeded to spin madly in a circle. Its turning procedure was in an infinite loop, and so it wouldn't stop trying to turn. We rescued it when it spit one of its wheels into space. This group's members admitted that their testing time was wasted because they had not mastered the basics before hand. Experience Worthwhile: * an appreciation for thinking through a problem * a much needed break from Java * an appreciation a programming language is * insight into robotics * a chance to" fail miserably" and still get credit},
address = {New York, NY, USA},
author = {Wolz, Ursula},
journal = {SIGCSE Bull.},
keywords = {*file-import-10-04-21,education,robotics},
number = {1},
pages = {95----99},
publisher = {ACM Press},
title = {{Teaching design and project management with lego RCX robots}},
url = {citeulike-article-id:7046112 http://dx.doi.org/http://doi.acm.org/10.1145/366413.364551},
volume = {33},
year = {2001}
}
